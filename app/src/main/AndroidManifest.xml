<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="will.exemploservice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter> //Cria um intent-filter abrindo a tag
                <action android:name="android.intent.action.MAIN" /> //passa ele como MAIN no action
                <category android:name="android.intent.category.LAUNCHER" /> //e informa LAUNCHER no category
            </intent-filter> //fecha a tag do intent-filter
        </activity>

        <service android:enabled="true" android:name=".Servico" />

    </application>

</manifest>